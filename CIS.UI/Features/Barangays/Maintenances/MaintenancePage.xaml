<UserControl x:Class="CIS.UI.Features.Barangays.Maintenances.MaintenancePage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Features/Barangays/Maintenances/Offices/OfficeView.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Office" Source="/Features/Barangays/Maintenances/Offices/OfficeView.xaml"/>
                <mui:Link DisplayName="Setting" Source="/Features/Barangays/Maintenances/Settings/SettingView.xaml"/>
                <mui:Link DisplayName="Purposes" Source="/Features/Barangays/Maintenances/Purposes/PurposeListView.xaml"/>
                <mui:Link DisplayName="Incumbents" Source="/Features/Barangays/Maintenances/Incumbents/IncumbentListView.xaml"/>
                <!--<mui:Link DisplayName="Positions" /> Source="/Features/Barangays/Maintenances/Officers/PositionListView.xaml"/>-->
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
