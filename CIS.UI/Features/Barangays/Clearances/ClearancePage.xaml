<Page x:Class="CIS.UI.Features.Barangays.Clearances.ClearancePage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:mui="http://firstfloorsoftware.com/ModernUI"
      mc:Ignorable="d" 
      d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Features/Barangays/Clearances/Applications/ApplicationView.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Application" Source="/Features/Barangays/Clearances/Applications/ApplicationView.xaml"/>
                <mui:Link DisplayName="Archive" Source="/Features/Barangays/Clearances/Archives/ArchiveView.xaml"/>
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</Page>
