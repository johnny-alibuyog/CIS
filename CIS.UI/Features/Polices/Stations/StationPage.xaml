<UserControl x:Class="CIS.UI.Features.Polices.Stations.StationPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Features/Polices/Stations/StationView.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Station Configuration" Source="/Features/Polices/Stations/StationView.xaml"/>
                <mui:Link DisplayName="Officer List" Source="/Features/Polices/Stations/OfficerListView.xaml"/>
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
