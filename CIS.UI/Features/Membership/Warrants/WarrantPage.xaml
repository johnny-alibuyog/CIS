<UserControl x:Class="CIS.UI.Features.Membership.Warrants.WarrantPage"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:mui="http://firstfloorsoftware.com/ModernUI"
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300">
    <Grid Style="{StaticResource ContentRoot}">
        <mui:ModernTab Layout="List" SelectedSource="/Features/Membership/Warrants/MasterList/WarrantListView.xaml">
            <mui:ModernTab.Links>
                <mui:Link DisplayName="Master List" Source="/Features/Membership/Warrants/MasterList/WarrantListView.xaml" />
                <mui:Link DisplayName="Import" Source="/Features/Membership/Warrants/Imports/ImportView.xaml"/>
                <!--<mui:Link DisplayName="Export" />-->
            </mui:ModernTab.Links>
        </mui:ModernTab>
    </Grid>
</UserControl>
